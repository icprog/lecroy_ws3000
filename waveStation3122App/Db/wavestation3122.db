
record (stringin, "$(P):GetIDN")
{
    field(DESC, "Identification string")
    field(DTYP, "stream")
    field( INP, "@wavestation3122.proto getAll $(PORT)")
}

record (stringin, "_$(P):GetAll_")
{
    field(DESC, "Identification string")
    field(DTYP, "stream")
    field( INP, "@wavestation3122.proto getIDN($(P)) $(PORT)")
    field(PINI, "YES")
    field(FLNK, "$(P):GetIDN")
}

record (stringin, "$(P):Serial")
{
    field(DTYP, "Soft Channel")
}

record (stringin, "$(P):FwVers") {
    field(DTYP, "Soft Channel")
}


record (stringin, "$(P):SwVers")
{
    field(DTYP, "Soft Channel")
    
}


